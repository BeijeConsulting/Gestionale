<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 5.3//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-5.3.dtd">  
  
 <hibernate-mapping>  
  <class name="it.beije.gestionale.Dipendente" table="dipendenti">  
    <id name="id">  
     <generator class="identity"></generator>  
    </id>  
            
    <property name="nome"></property>
    <property name="cognome"></property>
    <property name="dataNascita"></property>
    <property name="luogoNascita"></property>
    <property name="sesso"></property>
    <property name="codiceFiscale"></property>
    <property name="numeroTelefono"></property>
    <property name="mail"></property>
            
  </class>  
            
 </hibernate-mapping>  